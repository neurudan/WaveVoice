{
    "name": "SV baselines timit",
    "method": "bayes",
    "metric": {
        "name": "accuracy",
        "goal": "maximize"
    },
    "parameters": {
        "config_name": {
            "value": "SV.json"
        },
        "run_name": {
            "value": "baseline (+DATASET.base+, s+DATASET.num_speakers+)"
        },
        

        "01;SWEEP;TRAINING;num_epochs": {
            "value": 1000
        },
        "01;SWEEP;TRAINING;steps_per_epoch": {
            "value": 100
        },


        "01;SWEEP;OPTIMIZER;lr": {
            "min": 1e-9,
            "max": 0.1
        },


        "01;SWEEP;MODEL;num_filters": {
            "min": 16,
            "max": 1000
        },


        "01;SWEEP;OUTPUT_DENSE;num_filters": {
            "min" : 16,
            "max" : 1000
        },


        "01;SWEEP;DATASET;type": {
            "value": "real"
        },
        "01;SWEEP;DATASET;base": {
            "value": "timit"
        },
        
        
        "01;SWEEP;DATASET;speaker_list": {
            "value": "timit_speakers_470_stratified.txt"
        }
    }
}